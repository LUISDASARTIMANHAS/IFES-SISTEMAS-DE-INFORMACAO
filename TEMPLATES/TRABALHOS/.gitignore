# EditorConfig is awesome: https://EditorConfig.org 
 
# top-most EditorConfig file 
root = true 
 
[*] 
indent_style = space 
indent_size = 4 
end_of_line = crlf 
charset = utf-8 
trim_trailing_whitespace = false 
insert_final_newline = false 
 
# Arquivos padrão vscode e git 
.vscode 
.vscode/* 
.github 
 
# Ignorar arquivos de saída 
output/ 
output/* 
output 
 
# Ignorar arquivos JSON 
*.json 
 
# Ignorar arquivos BIN 
*.bin 
